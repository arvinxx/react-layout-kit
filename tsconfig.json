{
  "include": ["src/**/*.ts", "tests", ".dumi/**/*", ".dumirc.ts", "*.ts"],
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "lib": ["DOM", "ESNext"],
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["src/*"],
      "react-layout-kit": ["src"],
      "react-layout-kit/*": ["src/*", "*"]
    }
  }
}
